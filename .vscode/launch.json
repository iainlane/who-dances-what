    "version": "0.2.0",
    "configurations": [
        {
            "name": "dance-set",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/cmd/who-dances-what",
            "args": [
                "--db",
                "${workspaceFolder}/what2do.db",
                "dance-set",
                "YOUR NAMES HERE",
            ]
        },
        {
            "name": "list-dances",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${fileDirname}",
            "args": [
                "--db",
                "${workspaceFolder}/what2do.db",
                "list-dances"
            ]
        }
    ]
}
